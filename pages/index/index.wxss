  top: 2px;
  right: 5%;
  color: #ffffff;
}
.box-im{
  width: 80px;
  height: 80px;
  margin: auto;
  padding-top: 10px;
}
.im{
  width: 80px;
  height: 80px;
  border-radius: 100px;
}
