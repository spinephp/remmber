  height: 12px;
  float: left;
}
.tel{
  margin-top: 3px;
  color: #ffffff;
  display: block;
  text-align: center;
  font-size: 14px;
}
.midl{
  padding-top: 15px;
  height: 80px;
  overflow: hidden;
  background: #fff;
}
.imag image{
  width: 1px;
  height: 60px;
  float: left;
}
.mid{
  width: 32%;
  float: left;
}
.mid text{
  text-align: center;
  display: block;
  font-size: 14px;
}
.mid .mid-r{
  text-align: center;
  margin-top: 10px;
  color: #ffffff;
 
}
.mid-r view{
   border-radius: 50%;
   width: 32px;
   height: 32px;
   margin: auto;
   line-height: 32px;
   text-align: center;
}

.boo{
    position: fixed;
    width: 100%;
    top: 89%;
    height: 11%;
    overflow: hidden;
    background: #fff;
    font-family:"微软雅黑";
    font-size: 12px;
    -moz-box-shadow: 0px -1px 1px #dddddd;
    -webkit-box-shadow: 0px -1px 1px #dddddd;
    box-shadow:  0px -1px 1px #dddddd;
}
.boo navigator{
    margin-top: 2%;
    width:49%;
    float: left;
}

.boo navigator view{
    width: 27px;
    height: 27px;
    margin:auto;
}
.boo  navigator view image{
    width: 27px;
    height: 27px;
}
.boo  navigator text{
    display: block;
    text-align: center;
    margin-top: 4%;
}
