  width: 96%;
  background-size: 100% 100%;
}
.box_t{
  font-size: 16px;
  margin-left: 3%;
  height: 34px;
  line-height: 34px;
}
.box_p{
  font-size: 12px;
  color: #666666;
}
.p1{
  display: inline-block;
  margin-left: 3%;
  width: 40%;
  
}
.p2{
  display: inline-block;
  width: 28%;
