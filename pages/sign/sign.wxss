.box_main{
  background: #ffffff;
  padding-bottom: 30px;
  margin: 0 2% 0 2%;
  border-radius:0 0  5px 5px ;
}
.box{
  margin-left: 3%;
  overflow: hidden;
}
.box_m{
   float: left;
   margin-right: 2%;
   width: 17%;
   height: 80px;
   background-color: #f0f0f0;
   margin-top: 10px;
   border-radius: 8px;
   border: 1px solid #34dbbd;
}
.box_i{
   float: left;
   margin-right: 2%;
   width: 17%;
   height: 80px;
   background-color: #f0f0f0;
   margin-top: 10px;
   border-radius: 8px;
   border: 1px solid #f0f0f0;
}
.dui{
  height: 70px;
  background-repeat: no-repeat;
  background-position: right bottom;
  margin-top: 10px;  
  }
.box_img{
  width: 30px;
  height: 30px;
  border-radius: 50%;
  margin: 0 auto;
}
.im{
  width: 30px;
  height: 30px;
  border-radius: 50%;
}
.txt{
  font-size: 12px;
  text-align: center;
  display: block;
  height: 30px;
  line-height: 30px
}
.a{
  text-align: center;
  color: #34dbbd;
  text-decoration: underline;
  margin-top: 15px;
  font-size: 14px;
}
.btn{
  width: 92%;
  height: 40px;
  line-height: 40px;
  margin: 15px auto;
  background: #34dbbd;
  border: none;
  color: #ffffff;
}