.all{
  height: 130px;
  margin: 2% 2% 0 2%;
  background:#ffffff;
  border-radius: 5px;
  font-family:"微软雅黑";
}
.top{
  padding-left: 4%;
  overflow: hidden;
  font-size: 16px;
  height: 34px;
  line-height: 34px;
  border-bottom: 1px dashed #ebebeb;
}
.top text{
  float: left;
  display: block;
}
.right{
  float: right;
  padding-right:5px;
  width: 36%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.bottom{
  overflow: hidden;
}
.midl{
  margin-top: 5px;
  float: left;
  width: 66%;
}
.midl text{
  display: block;
  height: 22px;
  line-height: 22px;
  font-size: 14px;
  margin-left: 4%;
  color: #666666;
}
.you{
  float: right;
  margin-right: 20px;
  margin-top: 15px;
  width: 60px;
  height: 60px;
  border-radius: 30px;
  background: #ff8a20;
  color: #ffffff;
  font-size: 16px;
  line-height: 60px;
  text-align: center;
}
.t1{
  width: 36%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.t2{
  width: 100%;
   overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.t3{
  width: 100%;
   overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.t4{
  width: 100%;
   overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.te{
  width:46px;
  height:18px;
  line-height: 18px;
  background:#04bc00;
  border-radius: 2px;
  font-size:12px;
  text-align:center;
  margin-top:8px;
  margin-left: 10px;
  color: #ffffff;
}

/*下方菜单栏*/
.boo{
    position: fixed;
    width: 100%;
    top: 89%;
    height: 11%;
    overflow: hidden;
    background: #fff;
    font-family:"微软雅黑";
    font-size: 12px;
    -moz-box-shadow: 0px -1px 1px #dddddd;
    -webkit-box-shadow: 0px -1px 1px #dddddd;
    box-shadow:  0px -1px 1px #dddddd;
}
.boo navigator{
    margin-top: 2%;
    width:49%;
    float: left;
}

.boo navigator view{
    width: 27px;
    height: 27px;
    margin:auto;
}
.boo  navigator view image{
    width: 27px;
    height: 27px;
}
.boo  navigator text{
    display: block;
    text-align: center;
    margin-top: 4%;
}
.ig image{
  float: left;
  width: 1px;
  height: 56px;
}